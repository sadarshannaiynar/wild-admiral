{
    "search.exclude": {
        "# Library folders": true,
        "node_modules/": true,
        "# Logs": true,
        "logs/pm2logs/*.log": true,
        "logs/*.log": true,
        "# Build directories": true,
        "src/app/dist/public/": true,
