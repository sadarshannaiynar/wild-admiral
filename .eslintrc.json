{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "describe": true,
        "beforeEach": true,
        "context": true,
        "it": true,
        "fetch": true
    },
    "rules": {
        "import/no-extraneous-dependencies": 0
    },
    "settings" : {
        "import/resolver": {
            "webpack": {
                "config": "webpack.config.js"
            }
        }
    }
}