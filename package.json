{
  "name": "electron-react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for using Electron with React.js",
  "main": "src/app/index.js",
  "scripts": {
    "start:reactdev": "cross-env NODE_ENV=dev webpack-dev-server",
    "start:dev": "cross-env NODE_ENV=dev electron ./src/app/index.js",
    "build:prod": "cross-env NODE_ENV=prod webpack -p --progress --colors",
    "start": "cross-env NODE_ENV=prod electron ./src/app/index.js",
    "pretest": "rimraf coverage && eslint ./test/unit",
    "test": "cross-env NODE_ENV=test NODE_PATH=./src/client nyc --reporter=html --reporter=text mocha",
    "test:watch": "yarn test; chokidar './src/client/**/*.{js,jsx}' './test/unit/**/*.{js,jsx}' -c 'yarn test'",
    "build:mac": "yarn build:prod && electron-builder --mac",
    "build:linux": "yarn build:prod && electron-builder --linux",
    "build:windows": "yarn build:prod && electron-builder --windows",
    "build:all": "yarn build:prod && electron-builder -mwl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadarshannaiynar/wild-admiral.git"
  },
  "keywords": [
    "electron",
    "react",
    "boilerplate",
    "redux",
    "node"
  ],
  "author": {
    "name": "Adarsh",
    "email": "Adarsh",
    "url": "Adarsh"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sadarshannaiynar/wild-admiral/issues"
  },
  "homepage": "https://github.com/sadarshannaiynar/wild-admiral#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-object-rest-spread": "^0.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron": "^1.7.8",
    "electron-builder": "^19.31.1",
    "enzyme": "2.9.1",
    "es6-promise": "^4.1.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "postcss": "^6.0.12",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "15.6.1",
